{
  "name": "react-password-mask",
  "version": "3.3.0",
  "description": "Show/hide the contents of a password field.",
  "main": "lib/passwordMask.min.js",
  "scripts": {
    "build": "webpack",
    "test": "jest",
    "test:update": "jest -u",
    "test:watch": "npm test -- --watchAll",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint src test",
    "preversion": "npm run build && git add -A lib",
    "example": "webpack-dev-server --config example/webpack.config.js",
    "example:build": "cross-env NODE_ENV=production webpack --config example/webpack.config.build.js"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testRegex": "(/test/.*|(\\.|/))\\.js?$",
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nandastone/react-password-mask.git"
  },
  "keywords": [
    "react",
    "component",
    "show",
    "hide",
    "reveal",
    "password",
    "input",
    "field",
    "mask"
  ],
  "author": "Zak Angelle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nandastone/react-password-mask/issues"
  },
  "homepage": "https://github.com/nandastone/react-password-mask#readme",
  "peerDependencies": {
    "prop-types": "^15.6.1",
    "react": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.5.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.1.0",
    "jest": "^22.4.3",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "sinon": "^4.4.8",
    "style-loader": "^0.20.3",
    "webpack": "^4.2.0",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "babel-preset-env": "^1.6.1"
  }
}
